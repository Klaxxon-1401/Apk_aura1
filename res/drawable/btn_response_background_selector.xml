<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ff07cad3" android:endColor="#ff12afd3" android:angle="270.0" android:type="linear" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#0a000000" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#ff0edbc7" android:endColor="#ff07cad3" android:angle="270.0" android:type="linear" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
</selector>
